input) {\n      id\n      email\n      profile {\n        fullName\n        companyName\n        telephone\n        websiteAddress\n      }\n    }\n  }\n`;\n\nconst Settings = () => {\n  const meData = useQuery(meQuery, { fetchPolicy: 'cache-and-network' });\n  const [executeMutation, res] = useMutation(settingMutation);\n  const me = meData.data ? meData.data.me : {};\n  console.log('me data', me);\n\n  return (\n    <Layout noContainer>\n      <Seo title=\"Settings\" description=\"Update User info\" />\n      <Header />\n      <section className=\"section\">\n        <div className=\"container\">\n          <Container className=\"columns\">\n            {/* <div className=\"column is-one-fifth\">\n          <Sidebar />\n        </div> */}\n            <div className=\"column\">\n              <MainColumn paddingtop=\"1rem\">\n                <Heading>Settings</Heading>\n                <div>\n                  <SettingsForm\n                    enableReinitialize\n                    initialValues={me}\n                    onSubmit={async (data) => {\n                      await executeMutation({ variables: { input: data } });\n                      swal('Settings updated');\n                    }}\n                  />\n                </div>\n                {res.error && (\n                  <Message type=\"error\">{res.error.message}</Message>\n                )}\n                {res.loading ? <Loading /> : null}\n              </MainColumn>\n            </div>\n          </Container>\n        </div>\n      </section>\n      <CopyRight />\n    </Layout>\n  );\n};\n\nexport default Settings;\n","/Users/gagan/Sites/website-reviews-system/src/pages/admin/Reviews.js",[],"/Users/gagan/Sites/website-reviews-system/src/pages/admin/ProjectSubscription.js",[],"/Users/gagan/Sites/website-reviews-system/src/pages/admin/ProjectDashboard.js",[],"/Users/gagan/Sites/website-reviews-system/src/pages/admin/ReviewStyles.js",[],"/Users/gagan/Sites/website-reviews-system/src/pages/admin/Help.js",["384"],"import React from 'react';\nimport { useQuery } from '@apollo/client';\nimport gql from 'graphql-tag';\n\nimport Layout from '../../components/Layout';\nimport Seo from '../../components/Seo';\nimport Header from '../../components/Header';\nimport Sidebar from '../../components/Sidebar';\nimport { Heading, Subtitle } from '../../components/elements';\nimport MainColumn from '../../components/MainColumn';\nimport CopyRight from '../../components/CopyRight';\n\nconst helpQuery = gql`\n  query support {\n    support {\n      id\n      name\n      embedCode\n    }\n  }\n`;\n\nconst Help = () => {\n  const supportData = useQuery(helpQuery, {\n    fetchPolicy: 'network-only',\n  });\n  const support = supportData.data ? supportData.data.support : [];\n\n  return (\n    <Layout noContainer>\n      <Seo title=\"Help\" description=\"Manage Help Content\" />\n      <Header />\n      <div className=\"columns\">\n        <div className=\"column is-one-fifth\">\n          <Sidebar />\n        </div>\n        <div className=\"column\">\n          <MainColumn>\n            <Heading>Help</Heading>\n            <div className=\"columns\">\n              {support.map((item) => (\n                <div className=\"column\" key={item.id}>\n                  <Subtitle>{item.name}</Subtitle>\n                  <section\n                    dangerouslySetInnerHTML={{ __html: item.embedCode }}\n                  />\n                </div>\n              ))}\n            </div>\n          </MainColumn>\n        </div>\n      </div>\n      <CopyRight />\n    </Layout>\n  );\n};\n\nexport default Help;\n","/Users/gagan/Sites/website-reviews-system/src/store/schema.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/elements/Heading.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/elements/Title.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/elements/InputGroup.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/elements/Button.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/elements/TextArea.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/elements/CheckBox.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/elements/Message.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/elements/ReactDropdown.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/elements/CreditCardInput.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/elements/SelectGroup.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/elements/Subtitle.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/elements/Loading.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/elements/ColorPicker.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/elements/Dropzone.js",["385","386","387"],"/* eslint-disable react/jsx-props-no-spreading */\nimport React, { useState, useEffect } from 'react';\nimport { useDropzone } from 'react-dropzone';\nimport { useMutation } from '@apollo/client';\nimport gql from 'graphql-tag';\nimport styled from 'styled-components';\nimport randomstring from 'randomstring';\n\nimport { uploadFile } from '../../utils/upload';\nimport Loading from './Loading';\nimport theme from '../../utils/theme';\n\nconst signedUploadUrlMutation = gql`\n  mutation signedUploadUrl(\n    $fileName: String!\n    $fileType: String!\n    $isPublic: Boolean\n  ) {\n    signedUploadUrl(\n      fileName: $fileName\n      fileType: $fileType\n      isPublic: $isPublic\n    ) {\n      signedUrl\n      fileUrl\n    }\n  }\n`;\n\nconst getColor = (props) => {\n  if (props.isDragAccept) {\n    return '#00e676';\n  }\n  if (props.isDragReject) {\n    return '#ff1744';\n  }\n  if (props.isDragActive) {\n    return '#2196f3';\n  }\n  return theme.primaryColor;\n};\n\nconst Container = styled.div`\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  padding: 20px;\n  border-width: 2px;\n  border-radius: 2px;\n  border-color: ${(props) => getColor(props)};\n  border-style: dashed;\n  background-color: #fafafa;\n  color: ${(props) => props.theme.primaryColor};\n  outline: none;\n  transition: border 0.24s ease-in-out;\n  margin-bottom: 20px;\n  cursor: pointer;\n`;\n\nconst MyDropzone = ({ onUpload, isPublic, children, handleLoading }) => {\n  const [loading, setLoading] = useState(false);\n  const [error, setError] = useState(undefined);\n  const [executeUploadMutation] = useMutation(signedUploadUrlMutation);\n\n  useEffect(() => {\n    if (handleLoading) {\n      handleLoading(loading);\n    }\n  }, [loading]);\n\n  const onDrop = (acceptedFiles) => {\n    setLoading(true);\n    setError(undefined);\n\n    if (acceptedFiles.length > 0) {\n      acceptedFiles.map(async (file) => {\n        try {\n          const variables = {\n            fileName: `${randomstring.generate(3)}-${file.name}`,\n            fileType: file.type,\n          };\n\n          // get signed url from aws s3\n          const signedUploadUrl = await executeUploadMutation({\n            variables: {\n              ...variables,\n              isPublic: isPublic || false,\n            },\n          });\n          if (signedUploadUrl.data.error) {\n            console.log('upload error', signedUploadUrl.data.error);\n            return;\n          }\n\n          const { signedUrl, fileUrl } = await signedUploadUrl.data\n            .signedUploadUrl;\n\n          // return;\n          const options = {\n            headers: {\n              'Content-Type': file.type,\n            },\n          };\n\n          const result = await uploadFile(signedUrl, file, options);\n          console.log('upload success', result);\n\n          // upload success\n          onUpload({ ...variables, url: fileUrl });\n          setLoading(false);\n          // eslint-disable-next-line no-shadow\n        } catch (error) {\n          console.log('upload error', error);\n          setLoading(false);\n        }\n      });\n    }\n  };\n\n  const onDropRejected = (data) => {\n    if (data.length > 0) {\n      setError('Please upload files smaller than 5mb');\n      setLoading(false);\n    }\n  };\n\n  const {\n    getRootProps,\n    getInputProps,\n    isDragActive,\n    isDragAccept,\n    isDragReject,\n  } = useDropzone({\n    onDrop,\n    onDropRejected,\n    maxSize: 5000000,\n  });\n\n  if (children) {\n    return (\n      <span {...getRootProps({ isDragActive, isDragAccept, isDragReject })}>\n        <input {...getInputProps()} />\n        {children}\n      </span>\n    );\n  }\n\n  return (\n    <Container {...getRootProps({ isDragActive, isDragAccept, isDragReject })}>\n      <input {...getInputProps()} />\n      <p>+ Drag n drop some files here</p>\n      <small>or click to select files</small>\n      {loading && <Loading />}\n      {error && <small className=\"has-text-danger\">{error}</small>}\n    </Container>\n  );\n};\n\nexport default MyDropzone;\n","/Users/gagan/Sites/website-reviews-system/src/components/Layout.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/Seo.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/CopyRight.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/Header.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/Sidebar.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/MainColumn.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/ProjectDashboardHero.js",[],"/Users/gagan/Sites/website-reviews-system/src/hooks/useProjectDetails.js",[],"/Users/gagan/Sites/website-reviews-system/src/hooks/useProjectUpdate.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/ReviewItem.js",["388"],"import React, { useState } from 'react';\nimport styled from 'styled-components';\nimport moment from 'moment';\n\nimport Rating from './Rating';\nimport { CommentReplyForm } from './forms';\n\nconst Wrapper = styled.div`\n  :nth-child(even) {\n    background-color: #f2f2f2;\n  }\n  .columns {\n    margin-bottom: 0;\n  }\n  button {\n    background: transparent;\n    border: none;\n    :focus:not(:active) {\n      box-shadow: none;\n    }\n  }\n`;\n\nconst CommentReplyWrapper = styled.div`\n  margin-top: 15px;\n`;\n\nconst UserReview = styled.div`\n  color: ${(props) => (props.color ? props.color : '#000000')};\n  font-size: ${(props) => (props.fontSize ? `${props.fontSize}px` : '16px')};\n`;\n\nconst ReviewItem = ({\n  review,\n  starsColor,\n  reviewAuthorColor,\n  reviewBodyColor,\n  reviewBodySize,\n  reviewAuthorSize,\n}) => {\n  const [active, setActive] = useState(false);\n  const toggle = () => {\n    setActive(!active);\n  };\n\n  console.log(active);\n  return (\n    <Wrapper className=\"columns\">\n      <div className=\"column\">\n        <div className=\"columns\">\n          <UserReview\n            className=\"column is-1\"\n            color={reviewAuthorColor}\n            fontSize={reviewAuthorSize}>\n            {review.personName}\n          </UserReview>\n          <UserReview className=\"column is-1\">{review.location}</UserReview>\n          <div className=\"column is-2\">\n            {' '}\n            {moment(review.createdAt).format('Do MMM YYYY')}\n          </div>\n          <div className=\"column is-1\">\n            <Rating\n              disabled\n              count={5}\n              value={review.rating}\n              activeColor={starsColor}\n            />\n          </div>\n          <div className=\"column is-4\" />\n          <div className=\"column is-1\">\n            <button\n              className=\"button has-text-danger has-text-weight-bold\"\n              type=\"button\">\n              Delete\n            </button>\n          </div>\n          <div className=\"column is-1\">\n            <button className=\"button has-text-weight-bold\" type=\"button\">\n              Live\n            </button>\n          </div>\n          <div className=\"column is-1\">\n            <button\n              className=\"button has-text-success has-text-weight-bold\"\n              type=\"button\">\n              Submit\n            </button>\n          </div>\n        </div>\n        <div className=\"columns\">\n          <div className=\"column\">\n            <UserReview\n              className=\"is-size-7\"\n              color={reviewBodyColor}\n              fontSize={reviewBodySize}>\n              {review.comment}\n            </UserReview>\n          </div>\n        </div>\n        <a onClick={toggle} className=\"has-text-weight-bold\">\n          {active ? '- Reply' : '+ Reply'}\n        </a>\n        {active ? (\n          <CommentReplyWrapper>\n            <CommentReplyForm />\n          </CommentReplyWrapper>\n        ) : null}\n      </div>\n    </Wrapper>\n  );\n};\n\nexport default ReviewItem;\n","/Users/gagan/Sites/website-reviews-system/src/components/ProgressBar.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/PaymentConfirmation.js",[],"/Users/gagan/Sites/website-reviews-system/src/utils/upload.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/AuthFooter.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/AuthHeader.js",[],"/Users/gagan/Sites/website-reviews-system/src/hooks/useProjectGuestDetails.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/UploadImageModal.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/Rating.js",[],"/Users/gagan/Sites/website-reviews-system/src/utils/helpers.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/AdminUsers.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/SliderCarousal.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/Footer.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/forms/index.js",[],"/Users/gagan/Sites/website-reviews-system/src/components/forms/AdminUsersForm.js",[],"/Users/gagan/Sites/website-reviews-system/src/compo